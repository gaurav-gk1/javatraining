public class PatternUsingSingleFor {

	public static void main(String[] args) {
		int i;
		for (i=1;i<=5;i++) {
			switch(i) {
			case 1:
				System.out.println(i+" "+"5");
				break;
			case 2:
				System.out.println(i +" "+"4");
				break;
			case 3:
				System.out.println(i+" "+ "3");
				break;
			case 4:
				System.out.println(i +" "+"2");
				break;
			case 5:
				System.out.println(i +" "+"1");
			
			}
		}
